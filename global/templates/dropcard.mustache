<div class="drop-card">
    <div class="content">
        <div class="top-line">
            <div class="line="></div>
        </div>
        <div class="menu">
            <div class="left-column column">
                <div class="header">{{headerleft}}</div>
                <div class="links">
                    {{#links}}
                        <div class="link">
                            <i class="icon icon-right164"></i>
                            <a class="text">{{.}}</a>
                        </div>
                    {{/links}}
                </div>
            </div>
            <div class="right-column column">
                <div class="header">{{headerright}}</div>
                <div class="alphabet">
                    {{#alphabet}}
                        <div class="char">{{.}}</div>
                    {{/alphabet}}

                </div>
                <div class="header">{{headerrightsearch}}</div>
                <div class="search">
                    <input type="text" class="search-input" placeholder="{{searchplaceholder}}">
                    <button class="search-button">OK</button>
                </div>
            </div>
        </div>

    </div>
</div>
